{
  "manifest_version": 2,

  "name": "Omdb Extension",
  "description": "This extension has access to a film database",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]

}
